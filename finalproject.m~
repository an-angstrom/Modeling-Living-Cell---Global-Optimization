
function[]=finalproject()

data = importdata('onesequence_-22.79.dat',' ',1);
%P at 1,3,9,12,14,16,17,18,21,22,23,24,25,26
pos = data.data;
Lambda=0.9999;
T = 100;
count = 0;

while T>0.001
    
    count = count + 1;
    [V,pos] = MCMC(3,T,pos);
    T = T*Lambda;
    
end
count
V
finalenergy = graddescent(pos)




%writepdb
%step 1 
%file name string 


end

